<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SlideBottomPanel">
        <attr name="sbp_background_layout" format="reference"/>
        <attr name="sbp_title_height_no_display" format="dimension"/>
        <attr name="sbp_panel_height" format="dimension"/>
        <attr name="sbp_boundary" format="boolean"/>
        <attr name="sbp_move_distance_trigger" format="dimension"/>
        <attr name="sbp_animation_duration" format="integer"/>
        <attr name="sbp_fade" format="boolean"/>
        <attr name="sbp_hide_panel_title" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="MultiLineRadioGroup">
        <attr name="child_margin_horizontal" format="dimension" />
        <attr name="child_margin_vertical" format="dimension" />
        <attr name="child_layout" format="integer" />
        <attr name="child_count" format="integer" />
        <attr name="child_values" format="integer" />
        <attr name="single_choice" format="boolean" />
        <attr name="gravity" format="integer" />
    </declare-styleable>
</resources>